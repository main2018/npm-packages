{
  "name": "vite-plugin-svelte-imgsrc2import",
  "version": "0.1.23",
  "private": false,
  "license": "MIT",
  "description": "vite plugin to convert svelte img src to import",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/main2018/npm-packages.git"
  },
  "bugs": {
    "url": "https://github.com/main2018/npm-packages/issues"
  },
  "homepage": "https://github.com/main2018/npm-packages/tree/main/vite-plugin-svelte-imgsrc2import",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./dist/*"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "rollup -c",
    "prepublish": "npm version patch -m \"update: update version to %s\"",
    "publish": "git add . && git commit -am 'update: update version'",
    "postpublish": "git push && git push origin --tags",
    "pub": "npm run build && npm version patch --no-git-tag-version && npm publish --registry=https://registry.npmjs.org/",
    "postpub": "node ./postpub.js",
    "postpub1": "git add . && git commit -am 'update' && git push"
  },
  "keywords": [
    "vite",
    "plugin",
    "vite-plugin",
    "svelte",
    "img",
    "src",
    "import",
    "rollup",
    "vite-plugin",
    "vite-plugin-svelte",
    "vite-plugin-svelte-imgsrc2import",
    "vite-plugin-svelte-imgsrc2import",
    "imgsrc2import",
    "convert"
  ],
  "author": {
    "name": "椰子.Zz",
    "email": "375277023@qq.com"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@sveltejs/vite-plugin-svelte": ">=3.0.0",
    "svelte": ">=3.0.0",
    "vite": ">=6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/plugin-transform-runtime": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@babel/runtime": "^7.26.9",
    "@babel/runtime-corejs3": "^7.27.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/pluginutils": "^5.1.4",
    "rollup": "^4.35.0",
    "rollup-plugin-clear": "^2.0.7",
    "vite": "^6.0.6"
  }
}
