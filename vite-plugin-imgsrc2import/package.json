{
  "name": "vite-plugin-svelte-imgsrc2import",
  "version": "0.0.6",
  "private": false,
  "license": "MIT",
  "description": "vite plugin to convert svelte img src to import",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./dist/*"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "rollup -c",
    "pub": "npm run build && npm version patch --no-git-tag-version && npm publish --registry=https://registry.npmjs.org/",
    "postpub": "node ./postpub.js",
    "postpub1": "git add . && git commit -am 'update' && git push"
  },
  "keywords": [
    "vite",
    "plugin",
    "vite-plugin",
    "svelte",
    "img",
    "src",
    "import",
    "rollup",
    "vite-plugin",
    "vite-plugin-svelte",
    "vite-plugin-svelte-imgsrc2import",
    "vite-plugin-svelte-imgsrc2import",
    "imgsrc2import",
    "convert"
  ],
  "author": {
    "name": "椰子.Zz",
    "email": "375277023@qq.com"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "vite": ">=6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.35.0",
    "rollup-plugin-clear": "^2.0.7",
    "vite": "^6.0.6"
  }
}
